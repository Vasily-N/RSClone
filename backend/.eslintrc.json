{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
      "import/extensions": "off",
      "import/no-unresolved": "off",
      "class-methods-use-this": "off",
      "no-unused-vars": "off",
      "no-console": "off",
      "consistent-return": "off",
      "max-len": "off"
    }
}
